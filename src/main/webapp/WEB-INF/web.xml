<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">

    <display-name>DSSP Example Web Application</display-name>
    
    <servlet>
        <servlet-name>StartServlet</servlet-name>
        <servlet-class>be.e_contract.dssp.example.StartServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>StartServlet</servlet-name>
        <url-pattern>/sign</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>LandingServlet</servlet-name>
        <servlet-class>be.e_contract.dssp.example.LandingServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LandingServlet</servlet-name>
        <url-pattern>/landing</url-pattern>
    </servlet-mapping>
    
</web-app>
